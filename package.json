{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-common": "^3.1.2",
    "@apollo/react-hooks": "^3.1.2",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "formik": "^2.0.1-rc.13",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "react": "^16.10.2",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.10.2",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "4.0.8",
    "styled-components": "^4.4.0",
    "yup": "^0.27.0"
