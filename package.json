{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo": "^2.16.3",
    "apollo-boost": "^0.4.3",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "formik": "^2.0.1-rc.12",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-apollo-hooks": "^0.5.0",
    "react-dom": "^16.8.6",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "3.1.0",
    "styled-components": "^4.3.2",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "schema:download": "apollo schema:download --endpoint=http://localhost:1337/graphql",
    "codegen:generate": "apollo codegen:generate --queries=./src/**/*.tsx --localSchemaFile=./schema.json --outputFlat --target=typescript ./src/schemaTypes.ts",
    "gen:types": "npm run schema:download && npm run codegen:generate"
  },

  "devDependencies": {
    "@types/graphql": "^14.2.3",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.14.14",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-router-dom": "^4.3.4",
    "@types/styled-components": "^3.0.2",
    "typescript": "^3.5.3"
  },
  "resolutions": {
    "graphql": "14.0.2"
  }
}
