{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.3.1",
    "graphql": "^14.2.1",
    "react": "^16.5.2",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "3.1.0",
    "styled-components": "^4.0.0-beta.11.1",
    "styled-normalize": "^8.0.6"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "schema:download": "apollo schema:download --endpoint=http://localhost:1337/graphql",
    "codegen:generate": "apollo codegen:generate --queries=./src/**/*.tsx --localSchemaFile=./schema.json --outputFlat --target=typescript ./src/schemaTypes.ts",
    "gen:types": "npm run schema:download && npm run codegen:generate"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.1",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.3",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^3.0.2",
    "apollo": "^2.9.0",
    "typescript": "^3.1.1"
  },
  "resolutions": {
    "graphql": "14.0.2"
  }
}
