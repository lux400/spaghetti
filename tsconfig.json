{
 "compilerOptions": {
  "sourceMap": true,
  "target": "es5",
  "jsx": "react",
  "allowSyntheticDefaultImports": true,
  "noImplicitAny": false,
  "module": "es6",
  "moduleResolution": "node",
  "experimentalDecorators": true,
  "declaration": false,
  "removeComments": true,
  "noImplicitReturns": true,
  "strict": true,
  "outDir": "build",
  "noUnusedLocals": false,
  "noUnusedParameters": false,
  "lib": [
    "es6",
    "es7",
    "dom"
  ],
  "baseUrl": ".",
    "paths": {
      "src/*": [
        "src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "build",
    "scripts",
    "acceptance-tests",
    "webpack",
    "jest",
    "src/setupTests.ts"
  ],
  "typeRoots": [
    "./src/types",
    "./node_modules/@types"
  ]
}
